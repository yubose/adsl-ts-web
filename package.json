    "@types/sinon": "^10.0.3",
    "@types/sinon-chai": "^3.2.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.1.0",
    "@types/webpack-env": "^1.16.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^8.1.1",
    "cpy": "^8.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.6",
    "del": "^4.1.1",
    "docusaurus": "^1.14.7",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.12",
    "esbuild-loader": "^2.13.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esm": "^3.2.25",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.2",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.3.1",
    "html2canvas": "^1.3.2",
    "inject-body-webpack-plugin": "^1.3.0",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^4.0.0",
    "meow": "^8.1.2",
    "mime": "^1.6.0",
    "mocha": "^8.2.1",
    "mutation-observer": "^1.0.3",
    "nock": "^13.1.4",
    "nodemon": "^2.0.9",
    "noodl-action-chain": "file:packages/noodl-action-chain",
    "noodl-common": "^0.0.45",
    "noodl-types": "file:packages/noodl-types",
    "noodl-ui": "file:packages/noodl-ui",
    "noodl-ui-dom": "file:packages/noodl-ui-dom",
    "noodl-ui-test-utils": "file:packages/noodl-ui-test-utils",
    "noodl-utils": "file:packages/noodl-utils",
    "prettier": "^2.3.1",
    "process": "^0.11.10",
    "rollup": "^2.59.0",
    "rollup-plugin-esbuild": "^4.6.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "single-line-log": "^1.1.2",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "snabbdom": "^0.7.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "twilio-video": "^2.15.0",
    "type-fest": "^2.3.4",
    "typedoc": "^0.22.9",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^4.4.0",
    "webpack-hot-middleware": "^2.25.0",
    "workbox-webpack-plugin": "^6.4.2",
    "ws": "^8.2.2",
    "yaml": "^2.0.0-8"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true,
      "node": true,
      "worker": true
    },
    "extends": [
      "plugin:@typescript-eslint/eslint-recommended",
      "eslint:recommended",
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "globals": {
      "Twilio": "readonly",
      "echarts": "readonly"
    },
    "ignorePatterns": [
      "**/*.js",
      "**/*.js.map",
      "build",
      "dist",
      "generated",
      "node_modules"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2021,
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "env": {
      "es6": true,
      "mocha": true,
      "node": true,
      "worker": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules"
          ]
        }
      }
    },
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ],
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-extra-semi": "off",
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-this-alias": "off",
      "@typescript-eslint/no-unused-vars": 1,
      "arrow-body-style": "off",
      "camelcase": "off",
      "class-methods-use-this": "off",
      "consistent-return": "off",
      "default-case": "off",
      "dot-notation": "off",
      "eqeqeq": 0,
      "import/export": "off",
      "import/extensions": "off",
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": "off",
      "import/prefer-default-export": "off",
      "max-classes-per-file": "off",
      "no-alert": "off",
      "no-await-in-loop": "off",
      "no-bitwise": "off",
      "no-console": "off",
      "no-empty-function": "off",
      "no-eval": "off",
      "no-floating-decimal": "off",
      "no-inner-declarations": "off",
      "no-lonely-if": "off",
      "no-nested-ternary": "off",
      "no-new": "off",
      "no-param-reassign": "off",
      "no-plusplus": "off",
      "no-restricted-globals": "off",
      "no-restricted-syntax": "off",
      "no-return-assign": "off",
      "no-shadow": "off",
      "no-template-curly-in-string": "off",
      "no-undef": "off",
      "no-underscore-dangle": "off",
      "no-unused-expressions": "off",
      "no-unused-vars": "off",
      "no-use-before-define": "off",
      "no-useless-catch": "off",
      "no-void": "off",
      "prefer-arrow-callback": "off",
      "prefer-const": "off",
      "prefer-destructuring": "off",
      "prefer-rest-params": "off",
      "prefer-template": "off",
      "prettier/prettier": 2
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules"
          ]
        }
      }
    }
  },
  "keywords": [],
  "license": "ISC",
  "name": "aitmed-noodl-web",
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "npm run lib:build && webpack --mode=production",
    "build:dedWorker": "esbuild src/worker/dedicatedWorker/dedicatedWorker.ts --outfile=public/dedicatedWorker.js --bundle --target=es2015 --format=iife --sourcemap",
    "build:deploy:chinaServer": "npm run build:test && bash chinaServer.sh",
    "build:deploy:noodltest": "npm run build:test && npm run deploy:noodltest",
    "build:deploy:stable": "npm run build:stable && npm run deploy:stable",
    "build:deploy:test": "npm run build:test && npm run deploy:test",
    "build:stable": "cross-env ECOS_ENV=stable NODE_ENV=production DEPLOYING=true npm run build",
    "build:swWorker": "esbuild src/worker/serviceWorker/serviceWorker.ts --outfile=public/serviceWorker.js --target=es2015 --platform=browser --format=iife --sourcemap",
    "build:test": "cross-env ECOS_ENV=test NODE_ENV=production DEPLOYING=true npm run build",
    "deploy:noodltest": "bash s3Deploy.sh build cadltest.aitmed.com",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "editor": "lerna exec --scope noodl-editor 'npm run start'",
    "lib:build": "cross-env USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --scope noodl-types build",
    "lib:start": "cross-env NODE_ENV=development USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --parallel start",
    "postbuild": "npm run build:dedWorker",
    "postinstall": "lerna exec --scope noodl-types --scope noodl-action-chain \"npm run build\"",
    "poststart": "npm run start:worker",
    "prebuild": "rm -rf dist build",
    "start": "webpack serve --port 3000 --mode=development",
    "start:editor": "cross-env NODE_ENV=development ECOS_ENV=TEST lerna exec --scope noodl-editor \"npm run start\"",
    "start:sample": "cross-env SAMPLE=true NODE_ENV=development HMR=true ECOS_ENV=TEST USE_DEV_PATHS=true webpack serve --port 3000 --mode=development",
    "start:stable": "cross-env NODE_ENV=production HMR=true ECOS_ENV=stable webpack serve --port 3000",
    "start:test": "cross-env NODE_ENV=development ECOS_ENV=test HMR=true USE_DEV_PATHS=true npm run start",
    "start:worker": "npm run build:dedWorker -- --watch",
    "sync": "node scripts/op sync",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test USE_DEV_PATHS=true ts-mocha --extensions ts --file src/setupTests.ts --watch --watch-files src src/**/*.test.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file src/setupTests.ts --watch --watch-files src",
    "test:ndom": "lerna exec --scope noodl-ui-dom \"npm run test\"",
    "test:ndom:file": "lerna exec --scope noodl-ui-dom \"npm run test:file\"",
    "test:ntil": "lerna exec --scope noodl-utils \"npm run test\"",
    "test:nui": "lerna exec --scope noodl-ui \"npm run test\"",
    "test:nui:file": "lerna exec --scope noodl-ui \"npm run test:file\"",
    "test:workers": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha -r ts-node/register -r jsdom-global --extensions ts --watch --watch-files \"src\" src/__tests__/worker/**/*.test.ts",
    "updatenoodl": "node scripts/update.js -u noodl -d",
    "updatesdk": "node scripts/update.js -u sdk -d"
  },
  "version": "1.0.1",
  "workspaces": [
    "packages/*"
  ]
}
