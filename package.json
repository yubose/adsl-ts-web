{
  "_id": "aitmed-noodl-web@1.0.1",
  "author": "Christopher Tran<christopher@aitmed.com>",
  "bin": {
    "nui": "./bin/cli.js"
  },
  "version": "1.0.1",
  "author": "Christopher Tran<christopher@aitmed.com>",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "postinstall": "lerna exec --scope noodl-types --scope noodl-action-chain --scope noodl-utils \"npm run build\"",
    "bootstrap": "lerna bootstrap --hoist",
    "prebuild": "rm -rf dist build",
    "build": "npm run lib:build && webpack --mode=production",
    "build:stable": "cross-env ECOS_ENV=stable NODE_ENV=production DEPLOYING=true npm run build",
    "build:test": "cross-env ECOS_ENV=test NODE_ENV=production DEPLOYING=true npm run build",
    "build:test:dev": "cross-env ECOS_ENV=test NODE_ENV=development DEPLOYING=true npm run build",
    "build:deploy:stable": "npm run build:stable && npm run deploy:stable",
    "build:deploy:test": "npm run build:test && npm run deploy:test",
    "build:deploy:noodltest": "npm run build:test && npm run deploy:noodltest",
    "build:deploy:chinaServer": "npm run build:test && bash chinaServer.sh",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "deploy:noodltest": "bash s3Deploy.sh build cadltest.aitmed.com",
    "editor": "lerna exec --scope noodl-editor 'npm run start'",
    "build:serve": "cross-env NODE_ENV=production ECOS_ENV=test webpack --mode=production && npm run serve",
    "serve": "serve build -p 3000",
    "start": "webpack serve --port 3000 --mode=development",
    "start:editor": "cross-env NODE_ENV=development ECOS_ENV=TEST lerna exec --scope noodl-editor \"npm run start\"",
    "start:sample": "cross-env SAMPLE=true NODE_ENV=development HMR=true ECOS_ENV=TEST USE_DEV_PATHS=true webpack serve --port 3000 --mode=development",
    "start:stable": "cross-env NODE_ENV=production HMR=true ECOS_ENV=stable webpack serve --port 3000",
    "start:test": "cross-env NODE_ENV=development NODE_OPTIONS=\"--stack-trace-limit=10000\" ECOS_ENV=test HMR=true USE_DEV_PATHS=true npm run start",
    "start:test-diff-port": "cross-env NODE_ENV=development NODE_OPTIONS=\"--stack-trace-limit=10000\" ECOS_ENV=test HMR=true USE_DEV_PATHS=true webpack serve --port 4000 --mode=development",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file src/setupTests.ts --watch --watch-files src src/**/*.test.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file src/setupTests.ts --watch --watch-files src",
    "test:nui": "lerna exec --scope noodl-ui \"npm run test\"",
    "test:nui:file": "lerna exec --scope noodl-ui \"npm run test:file\"",
    "test:ndom": "lerna exec --scope noodl-ui-dom \"npm run test\"",
    "test:ndom:file": "lerna exec --scope noodl-ui-dom \"npm run test:file\"",
    "test:ntil": "lerna exec --scope noodl-utils \"npm run test\"",
    "lib:build": "cross-env USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --scope noodl-types build",
    "lib:start": "cross-env NODE_ENV=development USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --parallel start",
    "sync": "node scripts/op sync",
    "updatesdk": "node scripts/update.js -u sdk -d",
    "updatenoodl": "node scripts/update.js -u noodl -d"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "date-fns": "^2.21.2",
    "firebase": "^8.7.0",
    "idb-keyval": "^6.0.3",
    "lodash": "^4.17.20",
    "logsnap": "^0.0.21",
    "qrcode": "^1.4.4",
    "signature_pad": "^3.0.0-beta.4",
    "tippy.js": "^6.3.1",
    "util": "^0.12.3",
    "vercel-toast": "^1.5.3"
  },
  "description": "",
