  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build:stable": "cross-env ECOS_ENV=stable \"npm run build\"",
    "build:test": "cross-env ECOS_ENV=test \"npm run build\"",
    "build:deploy:stable": "cross-env ECOS_ENV=stable \"npm run build\" \"npm run deploy:stable\"",
    "build:deploy:test": "cross-env ECOS_ENV=test \"npm run build\" \"npm run deploy:test\"",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "link": "npm link noodl-ui",
    "start": "webpack-dev-server --hot --mode=development",
    "start:stable": "cross-env ECOS_ENV=stable \"npm run start\"",
    "start:test": "cross-env ECOS_ENV=test \"npm run start\"",
    "tsc": "tsc --watch",
    "unlink": "npm unlink noodl-ui"
  },
  "dependencies": {
    "@aitmed/cadl": "^1.0.70",
    "@aitmed/ecos-lvl2-sdk": "^1.4.96",
    "immer": "^7.0.7",
    "noodl-ui": "0.0.2",
    "twilio-video": "^2.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/lodash": "^4.14.159",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.1"
