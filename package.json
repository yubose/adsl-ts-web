{
  "name": "aitmed-noodl-web",
  "version": "1.0.1",
  "author": "Christopher Tran<christopher@aitmed.com>",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build:stable": "cross-env ECOS_ENV=stable \"npm run build\"",
    "build:test": "cross-env ECOS_ENV=test \"npm run build\"",
    "build:deploy:stable": "cross-env ECOS_ENV=stable \"npm run build\" \"npm run deploy:stable\"",
    "build:deploy:test": "cross-env ECOS_ENV=test \"npm run build\" \"npm run deploy:test\"",
    "cypress:ci": "start-server-and-test start:test http://127.0.0.1:3000 \"./node_modules/.bin/cypress run\"",
    "cypress:open": "./node_modules/.bin/cypress open",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "link": "npm link noodl-ui",
    "lint": "eslint . --ext .ts",
    "start": "cross-env ECOS_ENV=test NODE_ENV=development webpack-dev-server",
    "start:stable": "cross-env ECOS_ENV=stable \"npm run start\"",
    "start:test": "cross-env ECOS_ENV=test \"npm run start\"",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" ts-mocha -r ts-node/register -r tsconfig-paths/register --extensions ts --watch --watch-files src 'src/**/*.test.ts' --file src/setupTests.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" ts-mocha -r ts-node/register -r tsconfig-paths/register --extensions ts --watch --watch-files src --file ./src/setupTests.ts",
    "type-check": "tsc --noEmit --watch",
    "unlink": "npm unlink noodl-ui"
  },
  "dependencies": {
    "@aitmed/cadl": "^1.0.82",
    "@aitmed/ecos-lvl2-sdk": "^1.4.97",
    "@reduxjs/toolkit": "^1.4.0",
    "libphonenumber-js": "^1.7.57",
    "noodl-ui": "0.0.10",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "twilio-video": "^2.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@testing-library/cypress": "^7.0.0",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/user-event": "^12.1.0",
    "@types/chai": "^4.2.12",
    "@types/cypress": "^1.1.3",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.4",
    "@types/twilio-video": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "bundle-stats-webpack-plugin": "^2.2.0",
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "cypress": "^5.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.1.1",
    "prettier": "^2.1.1",
    "sinon": "^9.0.3",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    },
    "rules": {
      "chai-friendly/no-unused-expressions": "off",
      "no-template-curly-in-string": "off",
      "eqeqeq": 0
    }
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
