{
  "name": "aitmed-noodl-web",
  "version": "1.0.1",
  "author": "Christopher Tran<christopher@aitmed.com>",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "rm -rf build && webpack --mode=production",
    "build:stable": "cross-env ECOS_ENV=stable NODE_ENV=production npm run build",
    "build:test": "cross-env ECOS_ENV=test NODE_ENV=production npm run build",
    "build:deploy:stable": "cross-env ECOS_ENV=stable npm run build npm run deploy:stable",
    "build:deploy:test": "cross-env ECOS_ENV=test npm run build && npm run deploy:test",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "start": "cross-env ECOS_ENV=test NODE_ENV=development webpack-dev-server",
    "start:stable": "cross-env ECOS_ENV=stable NODE_ENV=development webpack-dev-server",
    "start:test": "cross-env ECOS_ENV=test npm run start",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha -r jsdom-global/register -r ts-node/register -r tsconfig-paths/register --extensions ts --watch --watch-files src 'src/**/*.test.ts' --file src/setupTests.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha -r jsdom-global/register -r ts-node/register -r tsconfig-paths/register --extensions ts --watch --watch-files src --file ./src/setupTests.ts",
    "tsc": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@aitmed/cadl": "^1.0.91",
    "@aitmed/ecos-lvl2-sdk": "^1.4.102",
    "noodl-ui": "0.0.24",
    "twilio-video": "^2.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.11.5",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/user-event": "^12.1.0",
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.4",
    "@types/twilio-video": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "bundle-stats-webpack-plugin": "^2.2.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.1.1",
    "prettier": "^2.1.2",
    "sinon": "^9.0.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    },
    "rules": {
      "chai-friendly/no-unused-expressions": "off",
      "no-template-curly-in-string": "off",
      "eqeqeq": 0
    }
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
