{
  "name": "aitmed-noodl-web",
  "version": "1.0.1",
  "author": "Christopher Tran<christopher@aitmed.com>",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "prebuild": "rm -rf dist build",
    "build": "npm run lib:build && webpack --mode=production",
    "build:stable": "cross-env ECOS_ENV=stable NODE_ENV=production DEPLOYING=true npm run build",
    "build:test": "cross-env ECOS_ENV=test NODE_ENV=production DEPLOYING=true npm run build",
    "build:deploy:stable": "npm run build:stable && npm run deploy:stable",
    "build:deploy:test": "npm run build:test && npm run deploy:test",
    "build:deploy:noodltest": "npm run build:test && npm run deploy:noodltest",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "deploy:cadltest": "bash s3Deploy.sh build cadltest.aitmed.com",
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.test.json",
    "start": "cross-env ECOS_ENV=test NODE_ENV=production HMR=true webpack-dev-server --port 3000 --mode=development",
    "start:stable": "cross-env ECOS_ENV=stable NODE_ENV=development webpack-dev-server --port 3000",
    "start:test": "cross-env ECOS_ENV=test USE_DEV_PATHS=true npm run start",
    "scripts:build": "webpack --mode production --config webpack.scripts.js",
    "scripts:start": "rm -rf scripts/compiled && webpack --mode development --config webpack.scripts.js --watch",
    "styleboard": "cross-env HMR=true rm -rf sbuild && webpack-dev-server --mode=development --config webpack.styles.config.js",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha -r jsdom-global/register --extensions ts --file src/setupTests.ts --watch --watch-files src src/**/*.test.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file src/setupTests.ts --watch --watch-files src",
    "test:noodlui": "lerna exec --scope noodl-ui \"npm run test\"",
    "test:noodlui:file": "lerna exec --scope noodl-ui \"npm run test:file\"",
    "test:noodlui:file:nosetup": "lerna exec --scope noodl-ui \"npm run test:file:nosetup\"",
    "test:noodluidom": "lerna exec --scope noodl-ui-dom \"npm run test\"",
    "test:noodluidom:file": "lerna exec --scope noodl-ui-dom \"npm run test:file\"",
    "test:actionchain": "lerna exec --scope noodl-ui \"npm run test:file src/__tests__/ActionChain.test.ts --verbose\"",
    "test:noodlutils": "lerna exec --scope noodl-utils \"npm run test\"",
    "test:noodlutils:file": "lerna exec --scope noodl-utils \"npm run test:file\"",
    "lib:build": "cross-env NODE_ENV=development USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --scope noodl-utils --parallel build",
    "lib:start": "cross-env NODE_ENV=development USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --scope noodl-utils --parallel start",
    "nodemon": "nodemon",
    "updatesdk": "node scripts/update.js -u sdk -d"
  },
  "dependencies": {
    "@aitmed/cadl": "^1.0.251",
    "@aitmed/ecos-lvl2-sdk": "^1.4.137",
    "axios": "^0.21.1",
    "date-fns": "^2.17.0",
    "firebase": "^8.2.7",
    "immer": "^9.0.1",
    "invariant": "^2.2.4",
    "lodash": "^4.17.20",
    "logsnap": "^0.0.21",
    "noodl-action-chain": "^0.0.52",
    "noodl-types": "^1.0.43",
    "noodl-ui": "file:packages/noodl-ui",
    "noodl-ui-dom": "file:packages/noodl-ui-dom",
    "noodl-utils": "file:packages/noodl-utils",
    "twilio-video": "^2.12.0",
    "vercel-toast": "^1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.12.16",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/user-event": "^13.1.1",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^9.0.1",
    "@types/invariant": "^2.2.34",
    "@types/jsdom-global": "^3.0.2",
    "@types/lodash": "^4.14.156",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.28",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@types/twilio-video": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "bundle-stats-webpack-plugin": "^2.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "commander": "^7.2.0",
    "concurrently": "^6.0.1",
    "copy-webpack-plugin": "^6.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.2",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^4.0.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "noodl-common": "^0.0.5",
    "prettier": "^2.1.2",
    "rollup": "^2.35.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.5.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.0.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^1.0.1",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "yaml": "^2.0.0-3"
  },
  "eslintConfig": {
    "globals": {
      "echarts": "readonly"
    },
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 7,
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "prettier/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules"
          ]
        }
      }
    },
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-extra-semi": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-this-alias": "off",
      "@typescript-eslint/no-unused-vars": 1,
      "arrow-body-style": "off",
      "consistent-return": "off",
      "default-case": "off",
      "dot-notation": "off",
      "no-param-reassign": "off",
      "camelcase": "off",
      "class-methods-use-this": "off",
      "eqeqeq": 0,
      "import/export": "off",
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "import/no-extraneous-dependencies": "off",
      "import/prefer-default-export": "off",
      "lines-between-class-members": "off",
      "max-classes-per-file": "off",
      "no-alert": "off",
      "no-await-in-loop": "off",
      "no-bitwise": "off",
      "no-console": "off",
      "no-empty-function": "off",
      "no-eval": "off",
      "no-void": "off",
      "no-new": "off",
      "no-inner-declarations": "off",
      "no-lonely-if": "off",
      "no-template-curly-in-string": "off",
      "no-nested-ternary": "off",
      "no-plusplus": "off",
      "no-use-before-define": "off",
      "no-unused-expressions": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-restricted-globals": "off",
      "no-restricted-syntax": "off",
      "no-return-assign": "off",
      "no-shadow": "off",
      "no-underscore-dangle": "off",
      "no-useless-catch": "off",
      "prettier/prettier": 2,
      "prefer-arrow-callback": "off",
      "prefer-const": "off",
      "prefer-destructuring": "off",
      "prefer-rest-params": "off",
      "prefer-template": "off"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
