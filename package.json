{
  "name": "aitmed-noodl-web",
  "version": "1.0.1",
  "author": "Christopher Tran<christopher@aitmed.com>",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build:stable": "cross-env ECOS_ENV=stable \"npm run build\"",
    "build:test": "cross-env ECOS_ENV=test \"npm run build\"",
    "build:deploy:stable": "cross-env ECOS_ENV=stable \"npm run build\" \"npm run deploy:stable\"",
    "build:deploy:test": "cross-env ECOS_ENV=test \"npm run build\" \"npm run deploy:test\"",
    "cypress:ci": "start-server-and-test start:test http://127.0.0.1:3000 \"cypress run\"",
    "cypress:open": "cypress open",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "link": "npm link noodl-ui",
    "start": "webpack-dev-server --hot --mode=development",
    "start:stable": "cross-env ECOS_ENV=stable \"npm run start\"",
    "start:test": "cross-env ECOS_ENV=test \"npm run start\"",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" ts-mocha --recursive -r ts-node/register --extensions ts '--watch --watch-files src src/**/*.test.ts' --file src/setupTests.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" ts-mocha -r ts-node/register --extensions ts --watch --watch-files src --file ./src/setupTests.ts",
    "type-check": "tsc --noEmit --watch",
    "unlink": "npm unlink noodl-ui"
  },
  "dependencies": {
    "@aitmed/cadl": "^1.0.74",
    "@aitmed/ecos-lvl2-sdk": "^1.4.96",
    "@reduxjs/toolkit": "^1.4.0",
    "history": "^5.0.0",
    "noodl-ui": "0.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "twilio-video": "^2.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/user-event": "^12.1.0",
    "@types/chai": "^4.2.12",
    "@types/cypress": "^1.1.3",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.4",
    "@types/twilio-video": "^2.7.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "cypress": "^4.12.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "html-webpack-plugin": "^4.3.0",
    "mocha": "^8.1.1",
    "sinon": "^9.0.3",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.0-dev.20200817",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:chai-friendly/recommended",
      "plugin:cypress/recommended"
    ],
    "env": {
      "cypress/globals": true
    },
    "plugins": [
      "cypress"
    ],
    "rules": {
      "chai-friendly/no-unused-expressions": "off",
      "no-template-curly-in-string": "off",
      "eqeqeq": 0
    }
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
