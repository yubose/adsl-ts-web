{
  "name": "aitmed-noodl-web",
  "version": "1.0.1",
  "author": "Christopher Tran<christopher@aitmed.com>",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "private": true,
  "bin": {
    "op": "node ./op"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "docs": "",
    "prebuild": "rm -rf dist build",
    "build": "npm run lib:build && webpack --mode=production",
    "build:stable": "cross-env ECOS_ENV=stable NODE_ENV=production DEPLOYING=true npm run build",
    "build:test": "cross-env ECOS_ENV=test NODE_ENV=production DEPLOYING=true npm run build",
    "build:deploy:stable": "npm run build:stable && npm run deploy:stable",
    "build:deploy:test": "npm run build:test && npm run deploy:test",
    "build:deploy:noodltest": "npm run build:test && npm run deploy:noodltest",
    "build:deploy:chinaServer": "npm run build:test && bash chinaServer.sh",
    "deploy:stable": "bash s3Deploy.sh build",
    "deploy:test": "bash s3Deploy.sh build devtest.aitmed.com",
    "deploy:cadltest": "bash s3Deploy.sh build cadltest.aitmed.com",
    "start": "webpack serve --port 3000 --mode=development",
    "start:stable": "cross-env NODE_ENV=production HMR=true ECOS_ENV=stable webpack serve --port 3000",
    "start:test": "cross-env NODE_ENV=development ECOS_ENV=test HMR=true USE_DEV_PATHS=true npm run start",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file src/setupTests.ts --watch --watch-files src src/**/*.test.ts",
    "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --file src/setupTests.ts --watch --watch-files src",
    "test:nui": "lerna exec --scope noodl-ui \"npm run test\"",
    "test:nui:file": "lerna exec --scope noodl-ui \"npm run test:file\"",
    "test:ndom": "lerna exec --scope noodl-ui-dom \"npm run test\"",
    "test:ndom:file": "lerna exec --scope noodl-ui-dom \"npm run test:file\"",
    "test:ntil": "lerna exec --scope noodl-utils \"npm run test\"",
    "lib:build": "cross-env USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --parallel build",
    "lib:start": "cross-env NODE_ENV=development USE_DEV_PATHS=true lerna run --scope noodl-ui --scope noodl-ui-dom --scope noodl-utils --parallel start",
    "updatesdk": "node scripts/update.js -u sdk -d",
    "updatenoodl": "node scripts/update.js -u noodl -d"
  },
  "dependencies": {
    "@jsmanifest/utils": "^1.0.12",
    "axios": "^0.21.1",
    "date-fns": "^2.21.2",
    "firebase": "^8.7.0",
    "invariant": "^2.2.4",
    "lodash": "^4.17.20",
    "logsnap": "^0.0.21",
    "noodl-action-chain": "^0.0.71",
    "noodl-ui": "file:packages/noodl-ui",
    "noodl-ui-dom": "file:packages/noodl-ui-dom",
    "noodl-utils": "^0.0.65",
    "process": "^0.11.10",
    "signature_pad": "^3.0.0-beta.4",
    "tippy.js": "^6.3.1",
    "util": "^0.12.3",
    "vercel-toast": "^1.5.3"
  },
  "devDependencies": {
    "@aitmed/cadl": "^1.0.350",
    "@aitmed/ecos-lvl2-sdk": "^1.4.154",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@fullcalendar/core": "^5.9.0",
    "@jsmanifest/typefest": "^1.0.9",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/user-event": "^13.1.9",
    "@types/chai": "^4.2.17",
    "@types/chai-as-promised": "^7.1.3",
    "@types/fs-extra": "^9.0.1",
    "@types/invariant": "^2.2.34",
    "@types/jsdom-global": "^3.0.2",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.0.3",
    "@types/node": "^15.14.0",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-env": "^1.16.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.2",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.2.6",
    "del": "^4.1.1",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.6",
    "esbuild-loader": "^2.13.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.2",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^4.0.0",
    "meow": "^8.1.2",
    "mime": "^1.6.0",
    "mocha": "^8.2.1",
    "mutation-observer": "^1.0.3",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "nodemon": "^2.0.9",
    "noodl-common": "^0.0.45",
    "noodl-types": "^1.0.146",
    "noodl-ui-test-utils": "file:packages/noodl-ui-test-utils",
    "noodl-webpack-plugin": "^0.0.13",
    "prettier": "^2.3.1",
    "rollup": "^2.52.7",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "single-line-log": "^1.1.2",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "twilio-video": "^2.15.0",
    "type-fest": "^1.1.3",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5",
    "typescript-plugin-css-modules": "^3.2.0",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "ws": "^7.5.0",
    "yaml": "^2.0.0-5"
  },
  "eslintConfig": {
    "globals": {
      "echarts": "readonly",
      "Twilio": "readonly"
    },
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 7,
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "plugin:@typescript-eslint/eslint-recommended",
      "eslint:recommended",
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true,
      "worker": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules"
          ]
        }
      }
    },
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-extra-semi": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-this-alias": "off",
      "@typescript-eslint/no-unused-vars": 1,
      "@typescript-eslint/lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ],
      "arrow-body-style": "off",
      "consistent-return": "off",
      "default-case": "off",
      "dot-notation": "off",
      "no-param-reassign": "off",
      "camelcase": "off",
      "class-methods-use-this": "off",
      "eqeqeq": 0,
      "import/export": "off",
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "import/no-extraneous-dependencies": "off",
      "import/prefer-default-export": "off",
      "max-classes-per-file": "off",
      "no-alert": "off",
      "no-floating-decimal": "off",
      "no-await-in-loop": "off",
      "no-bitwise": "off",
      "no-console": "off",
      "no-empty-function": "off",
      "no-eval": "off",
      "no-void": "off",
      "no-new": "off",
      "no-inner-declarations": "off",
      "no-lonely-if": "off",
      "no-template-curly-in-string": "off",
      "no-nested-ternary": "off",
      "no-plusplus": "off",
      "no-use-before-define": "off",
      "no-unused-expressions": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-restricted-globals": "off",
      "no-restricted-syntax": "off",
      "no-return-assign": "off",
      "no-shadow": "off",
      "no-underscore-dangle": "off",
      "no-useless-catch": "off",
      "prettier/prettier": 2,
      "prefer-arrow-callback": "off",
      "prefer-const": "off",
      "prefer-destructuring": "off",
      "prefer-rest-params": "off",
      "prefer-template": "off"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
