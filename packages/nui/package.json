{
  "author": "Christopher Tran <pfftdammitchris@gmail.com>",
  "description": "eCOS client-side NOODL JavaScript library",
  "devDependencies": {
    "esbuild": "^0.12.24",
    "esbuild-node-externals": "^1.3.0"
  },
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "homepage": "",
  "keywords": [
    "noodl",
    "ecos",
    "ui",
    "user",
    "interface",
    "nui"
  ],
  "license": "ISC",
  "main": "dist/nui.js",
  "name": "nui",
  "repository": {
    "type": "git",
    "url": "http://gitlab.aitmed.com/pfftdammitchris/aitmed-noodl-web.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node esbuild.config.js",
    "prebuild": "rm -rf dist",
    "start": "cross-env NODE_ENV=development node esbuild.config.js",
    "test": "npm run test:file \"src/**/*.test.ts\"",
    "test:file": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json -r jsdom-global/register --file src/__tests__/setupTests.ts --extensions ts --watch --watch-files \"src/**/*.ts\""
  },
  "version": "0.0.1"
}
