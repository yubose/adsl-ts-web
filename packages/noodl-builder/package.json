{
  "author": "Christopher Tran <pfftdammitchris@gmail.com>",
  "description": "JavaScript library to build noodl objects",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "homepage": "",
  "keywords": [
    "noodl",
    "ecos",
    "aitmed",
    "builder"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "name": "noodl-builder",
  "repository": {
    "type": "git",
    "url": "http://gitlab.aitmed.com/pfftdammitchris/aitmed-noodl-web.git"
  },
  "scripts": {
    "build": "concurrently \"rollup -c\" \"npm run types\"",
    "start": "concurrently \"rollup -c -w\" \"tsc --declaration --emitDeclarationOnly --declarationDir dist --skipLibCheck --esModuleInterop -w\"",
    "test": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json -r source-map-support/register --extensions ts --watch --watch-files \"src/**/*.ts\" \"src/**/*.test.ts\"",
    "types": "tsc --declaration --emitDeclarationOnly --declarationDir dist --skipLibCheck --esModuleInterop"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.2"
}
