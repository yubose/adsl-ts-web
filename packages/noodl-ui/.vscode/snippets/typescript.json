  },
  "chalk.blue": {
    "prefix": "chblue",
    "body": "chalk.blueBright('$1')"
  },
  "chalk.green": {
    "prefix": "chgreen",
    "body": "chalk.greenBright('$1')"
  },
  "chalk.yellow": {
    "prefix": "chyellow",
    "body": "chalk.yellowBright('$1')"
  },
  "chalk.magenta": {
    "prefix": "chmagenta",
    "body": "chalk.magentaBright('$1')"
  },
  "chalk.cyan": {
    "prefix": "chcyan",
    "body": "chalk.cyanBright('$1')"
  },
  "chalk.white": {
    "prefix": "chwhite",
    "body": "chalk.whiteBright('$1')"
