{
  "test": {
    "prefix": "test",
    "body": [
      "import { expect } from 'chai'",
      "import { screen } from '@testing-library/dom'"
    ]
  },
  "sinon": {
    "prefix": "sinon",
    "body": "import sinon from 'sinon'"
  },
