{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationDir": "dist",
    "esModuleInterop": true,
    "keyofStringsOnly": true,
    "lib": ["esnext"],
    "inlineSourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": true,
    "outDir": "dist",
    "strictNullChecks": true,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es2015"
  },
  "exclude": ["node_modules"],
  "files": ["src/index.ts"]
}
