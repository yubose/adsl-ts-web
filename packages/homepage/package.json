{
  "author": "Christopher <christopher@aitmed.com>",
  "bugs": {},
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "gatsby": "^4.4.0",
    "gatsby-plugin-emotion": "^7.4.0",
    "gatsby-plugin-gatsby-cloud": "^4.4.0",
    "gatsby-plugin-image": "^2.4.0",
    "gatsby-plugin-layout": "^3.6.0",
    "gatsby-plugin-manifest": "^4.4.0",
    "gatsby-plugin-offline": "^5.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-transformer-json": "^4.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "immer": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "description": "AiTmed's landing (home) page",
  "devDependencies": {
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "eslintConfig": {
    "overrides": [
      {
        "extends": [
          "plugin:@typescript-eslint/eslint-recommended",
          "plugin:@typescript-eslint/recommended",
          "prettier"
        ],
        "files": "**/*.+(ts|tsx)",
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
          "project": "./tsconfig.json",
          "sourceType": "module"
        },
        "plugins": [
          "@typescript-eslint/eslint-plugin"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": "off",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/no-unused-vars": [
            2,
            {
              "argsIgnorePattern": "^_"
            }
          ],
          "@typescript-eslint/no-use-before-define": [
            1
          ],
          "@typescript-eslint/no-var-requires": "off",
          "@typescript-eslint/quotes": [
            2,
            "backtick",
            {
              "avoidEscape": true
            }
          ],
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": [
                "**/*.test.ts",
                "**/*.spec.ts",
                "**/*.test.tsx",
                "**/*.spec.tsx"
              ]
            }
          ],
          "import/no-unresolved": 0,
          "no-use-before-define": [
            0
          ],
          "quotes": "off"
        }
      }
    ],
    "rules": {
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.test.[jt]s",
            "**/*.test.[jt]sx"
          ]
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "function-declaration"
        }
      ],
      "react/jsx-filename-extension": "off",
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": "off"
    },
    "settings": {
      "import/resolver": {
        "typescript": {
          "project": "."
        }
      },
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "cacheDirectory": "<rootDir>/.cache/unit",
    "globals": {
      "__PATH_PREFIX__": "",
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "@/(.*)": "<rootDir>/src/$1"
    },
    "preset": "ts-jest",
    "rootDir": ".",
    "setupFiles": [
      "<rootDir>/__helpers__/loadershim.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__helpers__/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/__mocks__/*",
      "node_modules",
      "\\.cache",
      "<rootDir>.*/public"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[tj]sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  },
  "keywords": [
    "aitmed",
    "ecos",
    "noodl",
    "telemedicine",
    "medical",
    "health",
    "ehealth"
  ],
  "license": "0BSD",
  "name": "homepage",
  "nodemonConfig": {
    "ignore": "*.json"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pfftdammitchris/aitmed.com.git"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deploy": "aws s3 rm s3://www.aitmed.com/static/web/latest/ --recursive && aws s3 cp public/ s3://www.aitmed.com/static/web/latest/ --recursive",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "serve": "gatsby serve -p 3000",
    "start": "gatsby develop -p 3000",
    "test": "jest"
  },
  "version": "0.1.0"
}
