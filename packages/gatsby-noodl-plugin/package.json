{
  "author": "Christopher <christopher@gmail.com>",
  "dependencies": {
    "loglevel": "^1.8.0"
  },
  "description": "Gatsby AiTmed plugin to generate static pages from a noodl config",
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "prettier"
    ],
    "ignorePatterns": [
      "**/*.js.map",
      "legacy",
      "dist",
      "output",
      "node_modules"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "plugins": [],
    "root": true,
    "rules": {
      "array-callback-return": "error",
      "arrow-parens": "warn",
      "arrow-spacing": "warn",
      "class-methods-use-this": "warn",
      "generator-star-spacing": "error",
      "import/export": "error",
      "no-alert": "off",
      "no-array-constructor": "error",
      "no-loop-func": "error",
      "no-new": "off",
      "no-new-func": "error",
      "no-plusplus": "warn",
      "no-shadow": "warn",
      "no-undef": "error",
      "no-unused-expressions": "warn",
      "no-useless-escape": "error",
      "prefer-const": "warn",
      "prefer-object-spread": "error",
      "prefer-spread": "warn",
      "prettier/prettier": 2,
      "space-before-blocks": "error"
    },
    "settings": {
      "react": {
        "version": "latest"
      }
    }
  },
  "keywords": [
    "aitmed",
    "website"
  ],
  "license": "0BSD",
  "main": "index.js",
  "name": "gatsby-noodl-plugin",
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "echo \"No build script setup\"",
    "test": "mocha --watch --watch-files . \"__tests__/**/*.test.js\""
  },
  "version": "1.0.0",
  "workspaces": [
    "./"
  ]
}
