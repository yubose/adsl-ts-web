{
  "author": "Christopher <christopher@aitmed.com>",
  "bugs": {},
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@theme-ui/css": "^0.14.7",
    "gatsby": "^4.7.2",
    "gatsby-core-utils": "^3.19.0",
    "gatsby-plugin-emotion": "^7.19.0",
    "gatsby-plugin-feed": "^4.19.0",
    "gatsby-plugin-gatsby-cloud": "^4.19.0",
    "gatsby-plugin-image": "^2.19.0",
    "gatsby-plugin-manifest": "^4.19.0",
    "gatsby-plugin-offline": "^5.19.0",
    "gatsby-plugin-react-helmet": "^5.19.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^4.19.0",
    "gatsby-plugin-sitemap": "^5.19.0",
    "gatsby-source-filesystem": "^4.19.0",
    "gatsby-transformer-json": "^4.19.0",
    "gatsby-transformer-sharp": "^4.19.0",
    "immer": "^8.0.1",
    "noodl-types": "^1.0.222",
    "noodl-ui": "^0.1.133",
    "noodl-utils": "^0.0.95",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-toastify": "^8.2.0",
    "theme-ui": "^0.14.2"
  },
  "description": "AiTmed's landing (home) page",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.5.0-alpha.64",
    "@storybook/addon-essentials": "^6.5.0-alpha.64",
    "@storybook/addon-interactions": "^6.5.0-alpha.64",
    "@storybook/addon-links": "^6.5.0-alpha.64",
    "@storybook/builder-webpack5": "^6.5.0-alpha.64",
    "@storybook/channel-websocket": "^6.4.22",
    "@storybook/manager-webpack5": "^6.5.0-alpha.64",
    "@storybook/react": "^6.5.0-alpha.64",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "babel-loader": "^8.2.5",
    "babel-preset-gatsby": "^2.12.1",
    "eslint-plugin-jest": "^26.1.4",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.26",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jsdom-global": "^3.0.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1"
  },
  "eslintConfig": {
    "overrides": [
      {
        "extends": [
          "prettier"
        ],
        "files": "./src/**/*.+(ts|tsx)",
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
          "project": "./tsconfig.json",
          "sourceType": "module"
        },
        "plugins": [
          "prettier",
          "jest"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": "off",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/no-use-before-define": [
            1
          ],
          "@typescript-eslint/no-var-requires": "off",
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": [
                "**/*.test.ts",
                "**/*.spec.ts",
                "**/*.test.tsx",
                "**/*.spec.tsx"
              ]
            }
          ],
          "import/no-unresolved": 0,
          "jest/no-disabled-tests": "warn",
          "jest/no-focused-tests": "error",
          "jest/no-identical-title": "error",
          "jest/prefer-to-have-length": "warn",
          "jest/valid-expect": "error",
          "no-use-before-define": [
            0
          ],
          "quotes": "off"
        }
      }
    ],
    "rules": {
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.test.[jt]s",
            "**/*.test.[jt]sx"
          ]
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "function-declaration"
        }
      ],
      "react/jsx-filename-extension": "off",
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": "off"
    },
    "settings": {
      "import/resolver": {
        "typescript": {
          "project": "."
        }
      },
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "cacheDirectory": "<rootDir>/.cache/unit",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/file-mock.js",
      "@/(.*)": "<rootDir>/src/$1",
      "^gatsby-page-utils/(.*)$": "<rootDir>/node_modules/gatsby-page-utils/dist/$1"
    },
    "rootDir": ".",
    "setupFiles": [
      "<rootDir>/jest/loadershim.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/jest/__mocks__/*",
      "node_modules",
      "src/__stories__/*",
      "src/__tests__/test-utils.tsx",
      "\\.cache",
      "<rootDir>.*/public"
    ],
    "testURL": "http://127.0.0.1",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/jest/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)",
      "\\.cache"
    ]
  },
  "keywords": [
    "aitmed",
    "ecos",
    "noodl",
    "telemedicine",
    "medical",
    "health",
    "ehealth"
  ],
  "license": "0BSD",
  "name": "static",
  "nodemonConfig": {
    "ignore": "*.json"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pfftdammitchris/aitmed.com.git"
  },
  "scripts": {
    "build": "gatsby build",
    "build-storybook": "build-storybook",
    "clean": "gatsby clean",
    "serve": "gatsby serve -p 3000",
    "start": "gatsby develop -p 3000",
    "storybook": "start-storybook -p 3005",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "version": "1.0.1"
}
