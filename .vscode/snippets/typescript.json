{
  "document.body.appendChild": {
    "prefix": "dbadd",
    "body": "document.body.appendChild($1)"
  },
  "document.body.removeChild": {
    "prefix": "dbremove",
    "body": "document.body.removeChild($1)"
  },
  "document.querySelector": {
    "prefix": "qs",
    "body": "document.querySelector(`$1`)"
  },
  "import app": {
    "prefix": "app",
    "body": "import app from 'App'"
  },
  "import page": {
    "prefix": "page",
    "body": "import page from 'Page'"
  },
  "import { Account }": {
    "prefix": "account",
    "body": "import { Account } from '@aitmed/cadl'"
  },
  "import { cadl } (unit test)": {
    "prefix": "tcadl",
    "body": "import { cadl } from 'setupTests'"
  },
  "import { noodl } (unit test)": {
    "prefix": "tnoodl",
    "body": "import { noodl } from 'setupTests'"
  },
  "import { cadl }": {
    "prefix": "cadl",
    "body": "import { cadl } from 'app/client'"
  },
  "import { noodl }": {
    "prefix": "noodl",
    "body": "import { noodl } from 'app/client'"
  },
  "import { forEachEntries }": {
    "prefix": "feentries",
    "body": "import { forEachEntries } from 'utils/common'"
  },
  "import { forEachDeepEntries }": {
    "prefix": "fedentries",
    "body": "import { forEachDeepEntries } from 'utils/common'"
  },
  "test": {
    "prefix": "test",
    "body": [
      "import { expect } from 'chai'",
      "import { coolGold, italic } from 'noodl-common'",
      "import * as mock from 'noodl-ui-test-utils'",
      "$1",
      "",
      "describe(coolGold(`$2`), () => {",
      "  xit(italic(`$3`), () => {",
      "    $4",
      "  })",
      "})",
      ""
    ]
  },
  "toast": {
    "prefix": "toast",
    "body": "import toast from 'vercel-toast'"
  },
  "logstable (func)": {
    "prefix": "logstable",
    "body": "isStable() && log.cyan(`$1`) $2"
  },
  "logstable (var)": {
    "prefix": "logstvar",
    "body": "stable && log.cyan(`$1`) $2"
  },
  "prettyDOM": {
    "prefix": "importpdom",
    "body": "import { prettyDOM } from '@testing-library/dom'"
  },
  "prettyDOM() log": {
    "prefix": "pdom",
    "body": "console.info(prettyDOM())"
  },
  "userEvent": {
    "prefix": "userevent",
    "body": "import userEvent from '@testing-library/user-event'"
  },
  "queryByDataKey": {
    "prefix": "qdatakey",
    "body": "import { queryByDataKey } from 'utils/test-utils'"
  },
  "queryByDataUx": {
    "prefix": "qdataux",
    "body": "import { queryByDataUx } from 'utils/test-utils'"
  }
}
