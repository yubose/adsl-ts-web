            "style": {
              "color": "0x000000ff",
              "left": "0.147",
              "top": "0.28",
              "width": "0.72",
              "height": "0.041",
              "fontSize": "24",
              "fontStyle": "bold",
              "display": "inline",
              "textAlign": {
                "x": "left",
                "y": "center"
