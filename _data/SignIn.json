          ".Global.currentUser.vertex.sk@": "=..formData.sk"
        },
        {
          "actionType": "popUp",
          "popUpView": "wrongPassword",
          "wait": true
        }
      ]
    },
    {
      "if": [
        {
          "=.builtIn.string.equal": {
            "dataIn": {
              "string1": "=.SignIn.formData.phoneNumber",
              "string2": "=.Global.phoneNumber"
            }
          }
        },
        "continue",
        {
